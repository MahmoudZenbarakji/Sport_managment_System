{
  "expo": {
    "name": "E-Commerce",
    "slug": "E-Commerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ecommerce",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/img/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#E9622b"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo.png",
        "backgroundColor": "#E9622b"
      },
      "splash": {
        "image": "./assets/img/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#E9622b"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E9622b"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your profile picture.",
          "cameraPermission": "The app accesses your camera to let you take a profile picture."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
